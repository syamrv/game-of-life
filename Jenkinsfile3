currentBuild.displayName = "Final_Demo # "+currentBuild.number

node('master'){
		
			def tag = sh returnStdout: true, script: 'git rev-parse HEAD'
	Docker_tag = tag()
		
	
		
		stage('ansible playbook'){
			sh 'echo ${Docker_tag}'
			 	script{
				    sh '''final_tag=$(echo $Docker_tag | tr -d ' ')
				     echo ${final_tag}test
				     echo $final_tag'''
				}
		
		}
	}
