currentBuild.displayName = "Final_Demo # "+currentBuild.number
node('master'){
	def tag(){
		def commit = sh(returnStdout: true, script: 'git rev-parse --short HEAD').trim()
		return commit
	}
	environment{
		Docker_tag = tag()
		}

		stage('ansible playbook'){
			sh 'echo ${Docker_tag}'
			 	script{
				    sh '''final_tag=$(echo $Docker_tag | tr -d ' ')
				     echo ${final_tag}test
				     echo $final_tag'''
				}
		
		}
	}
