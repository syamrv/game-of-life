currentBuild.displayName = "Final_Demo # "+currentBuild.number

node('master'){
		
	
	  environment {
        Docker_tag = sh(script: "printf \$(git rev-parse HEAD)", returnStdout: true)
    }
		
		stage('ansible playbook'){
			sh 'echo ${Docker_tag}'
			 	script{
				    sh '''final_tag=$(echo $Docker_tag | tr -d ' ')
				     echo ${final_tag}test
				     echo $final_tag'''
				}
		
		}
	}
