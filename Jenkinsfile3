currentBuild.displayName = "Final_Demo # "+currentBuild.number
def commit(){
        def tag = sh script: 'git rev-parse HEAD', returnStdout: true
		return tag
	}
node('master'){
	
	environment{
		Docker_tag = commit()
		}

		stage('ansible playbook'){
			sh 'echo ${Docker_tag}'
			 	script{
				    sh '''final_tag=$(echo $Docker_tag | tr -d ' ')
				     echo ${final_tag}test
				     echo $final_tag'''
				}
		
		}
	}
