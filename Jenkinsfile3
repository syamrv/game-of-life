currentBuild.displayName = "Final_Demo # "+currentBuild.number

node('master'){
    
    String getCommit() {
    return sh(script: 'git rev-parse HEAD', returnStdout: true)
}
Docker_tag = getCommit()

		
		stage('ansible playbook'){
			sh 'echo ${Docker_tag}'
			 	script{
				    sh '''final_tag=$(echo $Docker_tag | tr -d ' ')
				     echo ${final_tag}test
				     echo $final_tag'''
				}
		
		}
	}
